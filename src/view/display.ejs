<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <style>
    table,
    th,
    td {
      border: 2px solid black;
      border-collapse: collapse;
    }
  </style>
  <body>
    <table style="width: 100%">
      <tr>
        <th>Email</th>
        <th>FirstName</th>
        <th>LastName</th>
        <th>Address</th>
        <th>Action</th>
      </tr>
      <% for(var i=0; i < data.length; i++) { %>
      <tr>
        <td><%= data[i].email %></td>
        <td><%= data[i].firstName %></td>
        <td><%= data[i].lastName %></td>
        <td><%= data[i].address %></td>
        <td>
          <a href="/edit-user?id=<%=data[i].id%>">Edit</a>
          <a href="/delete-user?id=<%=data[i].id%>">Delete</a>
        </td>
      </tr>

      <% } %>
    </table>
  </body>
</html>
