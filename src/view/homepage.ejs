<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tran Dong</title>
</head>
<body>
    <h1>Hello world with Hoi Dan IT from view</h1>
    <ul>
        <% data.forEach(user => { %>
            <li><strong>ID:</strong> <%= user.id %> - <strong>Họ:</strong> <%= user.firstName %> - <strong>Tên:</strong> <%= user.lastName %> - <strong>Email:</strong> <%= user.email %></li>
        <% }) %>
    </ul>
    
</body>
</html>